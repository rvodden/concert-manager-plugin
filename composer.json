{
	"support" : {
		"email" : "richard@vodden.com"
	},
	"require" : {
		"php" : ">=7.0"
	},
	"name" : "rvodden/concert-management-plugin",
	"description" : "Wordpress plugin to manage concerts and similar semi-regular events",
	"homepage" : "http://www.vodden.com/",
	"license" : "GPL-3.0",
	"authors" : [{
			"name" : "Richard Vodden",
			"email" : "richard@vodden.com",
			"homepage" : "http://www.vodden.com/",
			"role" : "Creator"
		}
	],
	"keywords" : [
		"WordPress",
		"plugins",
		"music",
		"concert",
		"band",
		"ensemble"
	],
	"require-dev" : {
		"phpunit/phpunit" : "*",
		"respect/validation" : "*",
		"php-coveralls/php-coveralls" : "*",
		"10up/wp_mock" : "*",
		"wp-coding-standards/wpcs" : "*",
		"squizlabs/php_codesniffer" : "*",
		"dealerdirect/phpcodesniffer-composer-installer" : "*",
		"symfony/error-handler" : "*",
		"phing/phing" : "*",
		"roave/security-advisories" : "dev-master"
	},
	"autoload" : {
		"classmap": [
			"admin/",
			"common/",
			"public/",
			"tests/"
		]
	},
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
